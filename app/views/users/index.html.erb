<h3>Users List</h3>

<%= will_paginate @events %>

<table class="table table-hover table-bordered">
  <thead>
    <th>Name</th>
    <th>Gender</th>
  </thead>
  <% @users.each do |user| %>
  <tr>
    <td>
      <%= user.name %>
    </td>
    <td>
      <%= user.gender %>
    </td>
  </tr>
  <% end %>
</table>

<%= will_paginate @events %>
